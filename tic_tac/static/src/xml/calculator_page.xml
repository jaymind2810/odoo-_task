<template id="template" xml:space="preserve">

<t t-name="tic_tac_web">
    <div id="my_calculator_d" class="container">
    <div id="calculator">
        <h1>Calculator</h1>
        <div id="display-container">
            <p id="display"> </p>
        </div>
        <div id="buttons-container">
            <div class="button" id="button-clear" data-value="ac" t-on-click="this._onButtonPress">
                <span>AC</span>
            </div>
            <div class="button" id="button-sign" data-value="sign" t-on-click="this._onButtonPress">
                <span>+/-</span>
            </div>
            <div class="button" id="button-percent" data-value="%" t-on-click="this._onButtonPress">
                <span> % </span>
            </div>
            <div class="button red" data-value="/" id="button-divide" t-on-click="this._onButtonPress">
                <span> / </span>
            </div>
            <div class="button" id="button-seven" data-value="7" t-on-click="this._onButtonPress">
                <span>7</span>
            </div>
            <div class="button" id="button-eight" data-value="8" t-on-click="this._onButtonPress">
                <span>8</span>
            </div>
            <div class="button" id="button-nine" data-value="9" t-on-click="this._onButtonPress">
                <span>9</span>
            </div>
            <div class="button red" id="button-multiply" data-value="*" t-on-click="this._onButtonPress">
                <span>*</span>
            </div>
            <div class="button" id="button-four" data-value="4" t-on-click="this._onButtonPress">
                <span>4</span>
            </div>
            <div class="button" id="button-five" data-value="5" t-on-click="this._onButtonPress">
                <span>5</span>
            </div>
            <div class="button" id="button-six" data-value="6" t-on-click="this._onButtonPress">
                <span>6</span>
            </div>
            <div class="button red" id="button-minus" data-value="-" t-on-click="this._onButtonPress">
                <span>-</span>
            </div>
            <div class="button" id="button-one" data-value="1" t-on-click="this._onButtonPress">
                <span> 1 </span>
            </div>
            <div class="button" id="button-two" data-value="2" t-on-click="this._onButtonPress">
                <span> 2 </span>
            </div>
            <div class="button" id="button-three" data-value="3" t-on-click="this._onButtonPress">
                <span> 3 </span>
            </div>
            <div class="button red" id="button-plus" data-value="+" t-on-click="this._onButtonPress">
                <span> + </span>
            </div>
            <div class="button" id="button-zero" data-value="0" t-on-click="this._onButtonPress">
                <span> 0 </span>
            </div>
            <div class="button" id="button-decimal" data-value="." t-on-click="this._onButtonPress">
                <span> . </span>
            </div>
            <div class="button red" id="button-equals" data-value="=" t-on-click="this._onEqualButtonPress">
                <span> = </span>
            </div>
        </div>
    </div>
    </div>





